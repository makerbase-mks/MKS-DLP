     49          LCD_COLOR LCD_Index2Color_M444_12(int Index) {
     50            unsigned int r,g,b;
     51            /* Seperate the color masks */
     52            b = Index & 0xf;
     53            g = (Index >> 4) & 0xf;
     54            r = ((unsigned)Index >> 8) & 0xf;
     55            /* Convert the color masks */
     56            r = r * 17;
     57            g = g * 17;
     58            b = b * 17;
     59            return r + (g<<8) + (((U32)b)<<16);
