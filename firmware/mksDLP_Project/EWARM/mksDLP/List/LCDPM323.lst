     49          LCD_COLOR LCD_Index2Color_M323(int Index) {
     50            unsigned r, g, b;
     51            b = (Index & 7) * 255 / 7;
     52            g = ((Index >> 3) & 3) * 85;
     53            r = ((Index >> 5) & 7) * 255 / 7;
     54            return r + (g << 8) + (((U32)b) << 16);
